{
  "manifest_version": 3,
  "name": "Random Link Opener",
  "version": "1.0",
  "description": "Opens a random link from the current page",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
